{
  "name": "node-red-contrib-ccxt-v3",
  "version": "0.2.17",
  "description": "A Node-RED node to communicate with crypto exchanges using CCXT library supporting both CCXT Unified and Exchange-specific API calls.",
  "keywords": [
    "node-red",
    "ccxt",
    "bitcoin",
    "cryptocurrency",
    "trading",
    "cryptoexchange",
    "cryptotrading"
  ],
  "scripts": {
    "maps": "genmaps --sourceRoot /ccxt-v3/",
    "debug": "genmaps -r /ccxt-v3/ && node-red --inspect --nolazy",
    "start": "node-red",
    "test": "mocha \"test/**/*_spec.js\"",
    "lint": "eslint"
  },
  "author": "Eric Dahl",
  "license": "MIT",
  "dependencies": {
    "ccxt": "^4.2.4"
  },
  "engines": {
    "node": ">=14.0.0 <=21"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/skeyelab/node-red-contrib-ccxt-v3"
  },
  "bugs": "https://github.com/skeyelab/node-red-contrib-ccxt-v3/issues",
  "node-red": {
    "nodes": {
      "ccxt-api-v3": "ccxt-v3/ccxt-api.js"
    }
  },
  "devDependencies": {
    "ansicolor": "^1.1.93",
    "as-table": "^1.0.55",
    "ololog": "^1.1.164"
  },
  "contributors": [
    {
      "name": "Eric Dahl",
      "url": "https://github.com/skeyelab"
    },
    {
      "name": "Islam El'ewady",
      "url": "https://github.com/nileio"
    },
    {
      "name": "Miguel Gancedo",
      "url": "https://github.com/masalinas"
    },
    {
      "name": "Joel Wilson",
      "url": "https://github.com/LTangaF"
    },
    {
      "name": "Caleb",
      "url": "https://github.com/Borfswitch"
    }
  ]
}
